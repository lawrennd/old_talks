  author =	 {Neil D. Lawrence},
  ALTeditor = 	 {},
  title = 	 {The Atomic Human: Understanding Ourselves in the Age of AI},
  publisher = 	 {Allen Lane},
  year = 	 {2024},
  OPTkey = 	 {},
